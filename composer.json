{
    "name": "irando/country-codes",
    "description": "The most complete country code list fpackage for laravel",
    "homepage": "https://github.com/robertnicjoo/country-codes",
    "keywords": [
        "country",
        "country codes",
        "iso",
        "iso code",
        "laravel",
        "laravel8",
        "laravel9",
        "irando",
        "cv irando",
        "php"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Irando\\CountryCodes\\": "src/"
        }
    },
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Robert Nicjoo",
            "email": "robertnicjoo@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Irando\\CountryCodes\\CountryCodesServiceProvider"
            ]
        },
        "class": "Irando\\CountryCodes\\CountryCodesServiceProvider"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "composer/composer": "^2.0",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "illuminate/view": "^5.0|^6.0|^7.0|^8.0"
    },
    "prefer-stable": true
}
